{
  "name": "Requesty",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Requesty",
  "icons": {
    "512": "logo512.png"
  },
  "permissions": ["tabs"],
  "background": {
    "all_frames": true,
    "persistent": true,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "logo512.png"
  }
}
